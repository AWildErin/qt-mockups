QToolTip
{
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
		stop: 0 rgba(235, 234, 225, 15%),
		stop: 0.2 rgba(235, 234, 225, 70%),
		stop: 0.85 rgba(235, 234, 225,70%),
		stop: 1 rgba(112, 109, 73, 25%) );
	border-color: rgb(0,0,0, 25%);
	border-width: 1px;
	border-style: solid;
	padding: 2px;
	border-radius: 1px;
	opacity: 220;
}